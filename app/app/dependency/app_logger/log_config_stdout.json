{
    "version":1,
    "disable_existing_loggers":false,
    "formatters":{
        "detailed":{
            "class":"logging.Formatter",
            "format":"%(asctime)s: %(levelname)s: (%(name)s:%(module)s:%(funcName)s): %(message)s"
        }
    },
    "handlers":{
        "stdout":{
            "class":"logging.StreamHandler",
            "stream":"ext://sys.stdout",
            "formatter":"detailed"
        }
    },
    "loggers":{
        "uvicorn":{
            "handlers":[
                "stdout"
            ],
            "level":"DEBUG"
        },
        "api_service":{
            "handlers":[
                "stdout"
            ],
            "level":"DEBUG"
        }
    }
    
}